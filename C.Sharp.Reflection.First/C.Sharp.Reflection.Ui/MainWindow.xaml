<Window x:Class="C.Sharp.Reflection.Ui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:C.Sharp.Reflection.Ui"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid x:Name="search_grid">
        <TextBox x:Name="search_query_textbox" Margin="10,41,0,0" TextWrapping="Wrap" Text="" Height="23" VerticalAlignment="Top" HorizontalAlignment="Left" Width="395"/>
        <Button x:Name="search_query_button" Content="Search" Margin="0,41,10,0" Click="search_query_button_Click" HorizontalAlignment="Right" Width="97" Height="23" VerticalAlignment="Top"/>
        <Label x:Name="search_result_label" Content="" Margin="10,69,10,10"/>
        <Label x:Name="search_title_label" Content="Reflection Demo Search" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="26"/>
        <ComboBox x:Name="search_query_provider_combobox" HorizontalAlignment="Right" Margin="0,10,10,0" VerticalAlignment="Top" Width="355" Height="26"/>

    </Grid>
</Window>
