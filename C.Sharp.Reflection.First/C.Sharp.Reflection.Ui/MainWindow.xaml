<Window x:Class="C.Sharp.Reflection.Ui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:C.Sharp.Reflection.Ui"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid x:Name="search_grid">
        <TextBox x:Name="search_query_textbox" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" HorizontalAlignment="Left" Width="417"/>
        <Button x:Name="search_query_button" Content="Search" Margin="0,42,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="search_query_button_Click"/>
        <Label x:Name="search_result_label" Content="" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" Height="241" Width="497"/>
        <Label x:Name="search_title_label" Content="Reflection Demo Search" HorizontalAlignment="Center" Margin="10,10,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
